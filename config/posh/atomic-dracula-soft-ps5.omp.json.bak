{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "final_space": true,
  "console_title": true,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "powerline",
          "foreground": "#282a36",
          "background": "#ffb8d1"
        },
        {
          "type": "path",
          "style": "powerline",
          "foreground": "#f8f8f2",
          "background": "#5e6272",
          "properties": {
            "style": "folder",
            "home_icon": "~"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "foreground": "#f8f8f2",
          "background": "#b8a1f7"
        },
        {
          "type": "terraform",
          "style": "powerline",
          "foreground": "#1a1b26",
          "background": "#a2e8f2",
          "properties": {
            "display_mode": "context",
            "template": "üåç {{ .Workspace }}"
          }
        },
        {
          "type": "exit",
          "style": "powerline",
          "foreground": "#ffffff",
          "background": "#ff6e6e"
        }
      ]
    }
  ]
}
