{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "final_space": true,
  "console_title": true,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "powerline",
          "foreground": "#282a36",
          "background": "#ff79c6"
        },
        {
          "type": "path",
          "style": "powerline",
          "foreground": "#f8f8f2",
          "background": "#6272a4",
          "properties": {
            "style": "folder",
            "home_icon": "~"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "foreground": "#f8f8f2",
          "background": "#bd93f9"
        },
        {
          "type": "terraform",
          "style": "powerline",
          "foreground": "#282a36",
          "background": "#8be9fd",
          "properties": {
            "display_mode": "context",
            "template": "üåç {{ .Workspace }}"
          }
        },
        {
          "type": "exit",
          "style": "powerline",
          "foreground": "#f8f8f2",
          "background": "#ff5555"
        }
      ]
    }
  ]
}
